
#include <iostream>
using namespace std;

int main()
{
    double hours, minutes, time, ft;
	cout << "Enter Hours: ";
	cin >> hours;
	cout << endl << "Enter Minutes: ";
	cin >> minutes;
	time = hours + minutes / 60;
	ft = 4 * time*time / (time + 2) - 20;
	cout << endl << "Approximate Temperature: " << ft << endl;
	return 0;
}
